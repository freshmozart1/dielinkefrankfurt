  "main": "index.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node prisma/vercel-build.js && next build",
    "start": "next start",
    "check": "next lint && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --config jest.config.api.js",
    "test:api:watch": "jest --config jest.config.api.js --watch",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "seed:appointments": "npx ts-node-dev --transpile-only scripts/seed-appointments.ts",
    "seed:groups": "npx ts-node-dev --transpile-only scripts/seed-groups.ts",
    "seed:report": "npx ts-node-dev --transpile-only scripts/seed-status-reports.ts",
    "db:reset": "node prisma/reset-database.js",
    "db:push": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "db:migrate": "node prisma/reset-migrations.js",
    "db:generate": "prisma generate",
    "db:validate": "prisma validate",
    "db:start": "docker run --name postgres-dev -e POSTGRES_USER=devuser -e POSTGRES_PASSWORD=devpassword -e POSTGRES_DB=mydatabase -p 5432:5432 -d postgres",
    "db:stop": "docker stop postgres-dev && docker rm postgres-dev",
    "blob:start": "docker run --name vercel-blob-server --restart unless-stopped -p 9966:3000 -v ./dev/vercel-blob-store:/var/vercel-blob-store vercel-blob-server",
    "blob:stop": "docker stop vercel-blob-server && docker rm vercel-blob-server",
    "mail:dev": "maildev --web 1080 --smtp 1025"
  },
